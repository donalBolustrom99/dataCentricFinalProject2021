<h1>Modules</h1>
<table border="1">
    <tr>
        <th>Module ID</th>
        <th>Name</th>
        <th>Credits</th>
        <th>Update</th>
        <th>Students</th>
    </tr>
    <% modules.forEach((module)=>{ %>
        <tr>
            <td><%= module.mid %></td>
            <td><%= module.name %></td>
            <td><%= module.credits %></td>
            <td><a href="/modules/edit/:mid">Update</a></td>
            <td><a href="/">Students</a></td>
        </tr>
    <% }) %>
</table>
<a href="/">Home</a>