<h1>Lecturers</h1>
<form action="/addLecturer">
    <button>Add Lecturer</button>
</form>
<table border="1">
    <tr>
        <th>Lecturer ID</th>
        <th>Name</th>
        <th>Dept</th>
    </tr>
    <% lecturers.forEach((lecturer)=>{ %>
        <tr>
            <td><%= lecturer._id %></td>
            <td><%= lecturer.name %></td>
            <td><%= lecturer.dept %></td>
        </tr>
    <% }) %>
</table>
<a href="/">Home</a>